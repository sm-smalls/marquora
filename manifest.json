{
  "name": "Marquora",
  "version": "2.6",
  "description": "Quickly see, save, and hide questions on Quora",
  "background_page": "background.html",
  "browser_action": {
    "default_icon": "icon.png",
	"popup": "popup.html"
  },
  "permissions": [
	"tabs"
  ],
  "content_scripts": [
    {
	  "matches": [ "http://*.quora.com/*" ],
	  "js": ["contentscript.js"]
	}
  ]
}